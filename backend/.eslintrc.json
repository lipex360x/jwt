{
    "env": {
        "es2021": true,
        "node": true,
        "jest": true
    },
    "extends": [
        "standard"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
      "camelcase":"off",
      "no-undef":"off",
      "no-useless-constructor":"off",
      "no-useless-escape":"off",
      "prefer-promise-reject-errors":"off"
    }
}
